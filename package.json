    "ts-node": "~8.3.0",
    "typescript": "~4.1.5"
  },
  "lint-staged": {
    "*.{ts,html}": "eslint --cache --fix",
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write --ignore-unknown"
    ],
    "*.{scss,css}": [
      "stylelint --syntax scss --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
